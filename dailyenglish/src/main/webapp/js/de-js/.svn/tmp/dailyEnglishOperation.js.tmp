function display(){
	jUserLoginIn.getUserLoginSession( function(data) {
		if(data){
			$("#userIsNotLogin").hide();
			$("#userIsLogin").show();
			$("#userName").show();
			$("#userName").html(data);
			}else{
				$("#userName").html("");
				$("#userName").hide();
				$("#userIsLogin").hide();
			}
		});
}
	jUserLoginIn.getLoginUser(function(data){
		document.getElementById("welcome").innerHTML = "Welcome " + data;
	});
function userLogout(userName){
	//var b = $("#userName").text();
	jUserLoginIn.userLogout(userName,function(data) {
		$("#userName").html("");
		$("#userName").hide();
		$("#userIsNotLogin").show();
		$("#userIsLogin").hide();
		window.location = "dailyEnglishPage.html";
	});
}